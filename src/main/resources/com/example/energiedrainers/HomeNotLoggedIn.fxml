<VBox prefHeight="1011.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.energiedrainers.ControllerHomeNotLoggedIn">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="845.0" prefWidth="700.0" style="-fx-background-color: 6D6D6D;" VBox.vgrow="ALWAYS">
      <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#6d6d6d" height="414.0" layoutX="-3.0" layoutY="-6.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="716.0" />
            <Rectangle arcHeight="5.0" arcWidth="5.0" disable="true" fill="#d9d9d9" height="655.0" layoutX="11.0" layoutY="399.0" stroke="#d9d9d9" strokeType="OUTSIDE" strokeWidth="10.0" width="692.0" />

            <Label fx:id="statusLabel"/>
            <Button layoutX="257.0" layoutY="611.0" mnemonicParsing="false" prefHeight="68.0" prefWidth="242.0" style="-fx-background-radius: 20; -fx-background-color: #b1b1b1;" text="Inloggen" onAction="#handleLoginButtonAction">
               <font>
                  <Font name="Segoe UI Semibold" size="24.0" />
               </font>
            </Button>
          <Label fx:id="statusLabelTwo"/>
          <Button layoutX="257.0" layoutY="773.0" mnemonicParsing="false" prefHeight="68.0" prefWidth="241.0" style="-fx-background-color: B1B1B1; -fx-background-radius: 20;" onAction="#handleCreateAccountButtonAction" text="Account aanmaken">
